<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>View Staff</title>

    <style>
      .header {
        display: flex;
        justify-content: center;
      }
      .nav a {
        margin: 20px;
      }
      a {
        text-decoration: none;
        color: blue;
        font-size: 18px;
      }
    </style>
  </head>
  <body>
    <div class="header">
      <div class="nav">
        <a href="addstaff">Add Staff</a>
        <a href="addstudent">Add Student</a>
        <!-- <a href="addresult">Add Result</a> -->
        <a href="viewstaff">View Staff</a>
        <a href="view_result">View Result</a>
        <a href="viewstudent">View Student</a>
        <a href="addstd">Add STD</a>
        <a href="adddiv">Add Div</a>
      </div>
    </div>

      <h1 align="center">Staff Details</h1>
      <table align="center" border="1" cellpadding="5">
        <thead>
          <th>ID</th>
          <th>Name</th>
          <th>City</th>
          <th>Email</th>
          <th>Password</th>
        </thead>

        <tbody>
          <% result.forEach(function(staff){%>
          <tr class="align-middle">
            <td><%= staff.id%></td>
            <td><%= staff.name%></td>
            <td><%= staff.city%></td>
            <td><%= staff.email%></td>
            <td><%= staff.password%></td>
          </tr>
          <%})%>
        </tbody>
      </table>

  </body>
</html>
